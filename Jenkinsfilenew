pipeline {
    agent any
    
    stages {
        stage('clone') {
            steps {
              git credentialsId: 'gitlogin', url: 'https://github.com/puttagopi/java-tomcat-maven-example.git'
            }
            
        }
        stage('build') {
            steps {
                bat 'mvn clean install'
            }
        }
        
         stage('archive') {
            steps {
                archiveArtifacts artifacts: '**/*.war', followSymlinks: false
            }
        }
        
         stage('copy') {
            steps {
                echo 'copy'
            }
        }
        
         stage('deploy') {
            steps {
                build 'dev-tomact'
            }
        }
    }
}
